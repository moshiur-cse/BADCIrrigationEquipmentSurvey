@model IEnumerable<BADCIrrigationEquipmentSurvey.Models.DataModels.DistrictInfo>

@{
    ViewData["Title"] = "District Info";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@ViewData["Title"]</h2>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DivCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DivisionInfo.DivName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DistCode)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DistName)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.DivCode)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.DivisionInfo.DivName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.DistCode)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.DistName)
                </td>
                <td>
                    <a asp-action="Details" asp-route-id="@item.DistCode">Details</a>
                    @*<a asp-action="Edit" asp-route-id="@item.DistCode">Edit</a> |
                        <a asp-action="Details" asp-route-id="@item.DistCode">Details</a> |
                        <a asp-action="Delete" asp-route-id="@item.DistCode">Delete</a>*@
                </td>
            </tr>
        }
    </tbody>
</table>
